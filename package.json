{
  "name": "react-chat",
  "version": "0.1.0",
  "description": "An example of a simple web chat client built in ReactJS",
  "author": "TheStoneFox",
  "scripts": {
    "start": "node server.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thestonefox/react-chat"
  },
  "bugs": {
    "url": "https://github.com/thestonefox/react-chat/issues"
  },
  "devDependencies": {
    "6to5-jest": "^3.0.0",
    "jest-cli": "^0.3.0",
    "jsx-loader": "^0.12.2",
    "react-hot-loader": "^1.1.1",
    "react-tools": "^0.12.2",
    "webpack": "1.4.15",
    "webpack-dev-server": "1.6.4"
  },
  "dependencies": {
    "node-translate": "^0.0.4",
    "react": "^0.12.0"
  },
  "jest": {
    "rootDir": "./",
    "scriptPreprocessor": "<rootDir>/scripts/support/preprocessor.js",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
